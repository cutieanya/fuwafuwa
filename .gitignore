# Flutter/Dartの自動生成ファイル
.dart_tool/
.packages
.flutter-plugins
.flutter-plugins-dependencies

# ビルド成果物
build/

# IDEやエディタの設定ファイル
.idea/
*.iml
.vscode/

# 補助ツールの依存関係ファイル
# (Node.js/npmを補助的に使う場合)
node_modules/

# プラットフォーム固有のファイル
# Android
/android/.gradle/
/android/local.properties

# iOS
/ios/Pods/
/ios/Podfile.lock
/ios/.symlinks/
/ios/Runner/GeneratedPluginRegistrant.h
/ios/Runner/GeneratedPluginRegistrant.m
/ios/Runner.xcworkspace/
/ios/Runner.xcodeproj/project.xcworkspace/
/ios/Runner.xcodeproj/xcuserdata/

# macOS
/macos/Pods/
/macos/Podfile.lock
/macos/.symlinks/
/macos/Flutter/ephemeral/

# Web
# (Web用のビルド成果物はルートのbuild/に含まれるため、通常は個別指定不要)

# Windows
/windows/flutter/ephemeral/
/windows/runner/Debug/
/windows/runner/Profile/
/windows/runner/Release/

# Linux
/linux/flutter/ephemeral/

# テスト関連
coverage/

# ログファイル
*.log

# OS固有の自動生成ファイル
# macOS
.DS_Store
# Windows
Thumbs.db
desktop.ini


# ★★★ 特に重要：秘密情報ファイル ★★★
# .envファイル自体と、.env.developmentなど派生ファイルは無視
.env
.env.*
# ただし、テンプレートファイルは無視しない（Git管理に含める）
!/.env.example

# 実際のファイル名に合わせてください。例：
# lib/secrets.dart